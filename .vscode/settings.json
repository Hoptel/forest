{
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--ignore=E501"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "_config.yml": true,
        "requirements.txt": true,
        "README.md": true,
        "LICENSE": true,
        ".vscode": true,
        "*.sqlite": true,
        ".gitignore": true
    },
    "explorerExclude.backup": null
}